\begin_layout Standard
Substituting Eqn.
\begin_inset space ~
\end_inset


\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:knknm2appr2"
plural "false"
caps "false"
noprefix "false"

\end_inset

 into Eqn.
\begin_inset space ~
\end_inset


\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:knknm2appr1"
plural "false"
caps "false"
noprefix "false"

\end_inset

, we obtain
\begin_inset Formula 
\begin{equation}
\begin{aligned}r_{n-1}\frac{d\usf{K}_{n}}{dr}= & \left(\usf{A}_{n}^{-1}\usf{B}_{n}\usf{m}_{n-1}\usf{B}_{n}^{-1}\usf{A}_{n}-\usf{m}_{n}\right)\usf{K}_{n}\\
 & +\usf{A}_{n}^{-1}\left(2\usf{D}_{n}+\usf{B}_{n}\usf{m}_{n-1}\usf{B}_{n}^{-1}\usf{D}_{n-1}\right)+O(\Delta r).
\end{aligned}
\label{eq:AppendKndr1}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Taking the zero order of approximation of Eqn.
\begin_inset space ~
\end_inset


\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:AppendKndr1"
plural "false"
caps "false"
noprefix "false"

\end_inset

 around
\begin_inset space ~
\end_inset


\begin_inset Formula $r_{n}$
\end_inset

 and noting that
\begin_inset Formula 
\begin{equation}
\usf{B}_{n}\usf{m}_{n-1}\usf{B}_{n}^{-1}=\tilde{\usf{B}}_{n}\usf{m}_{n-1}\tilde{\usf{B}}_{n}^{-1},
\end{equation}

\end_inset

we have
\begin_inset Formula 
\begin{equation}
\begin{aligned}\begin{split}2r_{n}\frac{d\usf{K}_{n}}{dr}= & \left(\usf{r}_{n,n}^{-1}\tilde{\usf{A}}_{n}^{-1}\tilde{\usf{B}}_{n}\usf{m}_{n-1}\tilde{\usf{B}}_{n}^{-1}\tilde{\usf{A}}_{n}\usf{r}_{n,n}-\usf{m}_{n}\right)\usf{K}_{n}\\
 & +r_{n}^{2}\usf{r}_{n,n}^{-1}\tilde{\usf{A}}_{n}^{-1}\left(-2\usf{I}+\tilde{\usf{B}}_{n}\usf{m}_{n-1}\tilde{\usf{B}}_{n}^{-1}\right)\tilde{\usf{D}}_{n}+O(\Delta r),
\end{split}
\end{aligned}
\label{eq:AppendKndr2}
\end{equation}

\end_inset

where
\begin_inset space ~
\end_inset


\begin_inset Formula $\tilde{\usf{D}}_{n}$
\end_inset

 is a constant vector
\begin_inset Formula 
\begin{equation}
\tilde{\usf{D}}_{n}=\begin{bmatrix}\mu_{1,n}-\mu_{1,n-1}\\
\mu_{2,n}-\mu_{2,n-1}\\
Q_{5,n}-Q_{5,n-1}\\
W_{5,n}-W_{5,n-1}
\end{bmatrix}.
\end{equation}

\end_inset

Let
\end_layout

\begin_layout Standard
\begin_inset Preview

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{subequations}
\backslash
begin{align}
\backslash
usf{M}_n & :=
\backslash
tilde{
\backslash
usf{A}}^{-1}_n
\backslash
tilde{
\backslash
usf{B}}_n
\backslash
usf{m}_{n-1}
\backslash
tilde{
\backslash
usf{B}}^{-1}_n
\backslash
tilde{
\backslash
usf{A}}_n,
\backslash

\backslash

\backslash
usf{F}_n & :=
\backslash
tilde{
\backslash
usf{A}}^{-1}_n
\backslash
left( -2
\backslash
usf{I} +
\backslash
tilde{
\backslash
usf{B}}_n
\backslash
usf{m}_{n-1}
\backslash
tilde{
\backslash
usf{B}}^{-1}_n
\backslash
right)
\backslash
tilde{
\backslash
usf{D}}_n,
\backslash
end{align}
\backslash
end{subequations}
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
We rewrite Eqn.
\begin_inset space ~
\end_inset


\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:AppendKndr2"
plural "false"
caps "false"
noprefix "false"

\end_inset

 as
\begin_inset Formula 
\begin{equation}
2r_{n}\frac{d\usf{K}_{n}}{dr}=\left(\usf{r}_{n,n}^{-1}\usf{M}_{n}\usf{r}_{n,n}+\usf{m}_{n}\right)\usf{K}_{n}+r_{n}^{2}\usf{r}_{n,n}^{-1}\usf{F}_{n}.\label{eq:AppendKndr}
\end{equation}

\end_inset

Then multiplying Eqn.
\begin_inset space ~
\end_inset


\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:AppendKndr"
plural "false"
caps "false"
noprefix "false"

\end_inset

 by
\begin_inset space ~
\end_inset


\begin_inset Formula $\usf{r}_{n,n}$
\end_inset

 on both sides, we obtain a linear ODE system of
\begin_inset space ~
\end_inset


\begin_inset Formula $\usf{K}_{n}$
\end_inset


\begin_inset Formula 
\begin{equation}
2r_{n}\frac{d\pr{\usf{r}_{n,n}\usf{K}_{n}}}{dr}=\pr{\usf{M}_{n}+\usf{m}_{n}}\pr{\usf{r}_{n,n}\usf{K}_{n}}+r_{n}^{2}\usf{F}_{n},\label{eq:AppendKn}
\end{equation}

\end_inset

with boundary conditions given by
\begin_inset space ~
\end_inset


\begin_inset CommandInset ref
LatexCommand eqref
reference "eq:boundary"
plural "false"
caps "false"
noprefix "false"

\end_inset

.
\end_layout

\begin_layout Subsection
